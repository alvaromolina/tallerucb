<h1>Practicas</h1>
<%= Time.zone.now %>
<%= Time.new(2013,2,28,23,59,59) %>


<%= Time.zone.now > Time.new(2013,2,28,23,59,59,'-04:00')
 
%>
<table class="table table-striped">
  <tr>
    <th>Practica</th>
    <th>Usuario</th>
    <th>Email</th>
    <th>Soluciones</th>
    <th>Puntos</th>
    <th>Detail</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.practice %></td>
    <td><%= user.name %></td>
    <td><%= user.email %></td>
    <td><%= user.sol %></td>
    <td><%= user.points %></td>
    <td> <%= link_to 'detalle', '/solutions/report2/'+user.id.to_s+'/'+user.practice.to_s %></td>
  </tr>
<% end %>
</table>
